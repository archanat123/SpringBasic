<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="id2" class="sprinttest.student">
		<property name="name" value="abc"></property>
		<property name="marks" value="90"></property>
		<property name="address" ref="addr2"></property>
	</bean>
	<bean id="addr2" class="sprinttest.Address">
		<property name="city" value="sunnyvale"></property>
		<property name="state" value="CA"></property>
	</bean>
</beans>
